{
    "name": "dbould/markdown-escaper",
    "description": "Escapes Markdown characters so they get displayed as normal characters in a markdown document",
    "keywords": ["markdown", "escape"],
    "homepage": "http://github.com/dbould/markdown-escaper",
    "license": "MIT",
    "authors": [
        {
            "name": "Dave Bould",
            "email": "dave@bould.net",
            "homepage": "http://dave.codes"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Dbould\\MarkdownEscaper\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }

}
